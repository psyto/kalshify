# Kalshify Environment Variables

# Database
DATABASE_URL="postgresql://user:password@localhost:5432/kalshify"

# Authentication
AUTH_SECRET="your-auth-secret-here"
NEXTAUTH_URL="http://localhost:3000"

# Kalshi API
KALSHI_API_KEY="your-kalshi-api-key"
KALSHI_API_SECRET="your-kalshi-api-secret"
KALSHI_USE_SANDBOX="true"

# DFlow API (Solana)
DFLOW_API_KEY="your-dflow-api-key"
DFLOW_BUILDER_CODE="your-builder-code"
DFLOW_NETWORK="devnet"

# Anthropic (Claude AI)
ANTHROPIC_API_KEY="your-anthropic-api-key"

# Site URL
NEXT_PUBLIC_SITE_URL="http://localhost:3000"
